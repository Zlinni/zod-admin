{
  "name": "zod-admin",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "build-dev": "cross-env NODE_ENV=development vite build --mode development",
    "build-test": "cross-env NODE_ENV=test vite build --mode test",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .ts,.tsx,.vue --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky",
    "release": "standard-version --skip.commit"
  },
  "dependencies": {
    "@vee-validate/zod": "^4.13.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.2.1",
    "dayjs": "^1.11.10",
    "filesize": "^10.1.4",
    "highlight.js": "^11.9.0",
    "iconify-icon": "^2.1.0",
    "main-thread-scheduling": "^14.2.0",
    "mockjs": "^1.1.0",
    "numeral": "^2.0.6",
    "pinia": "^2.1.7",
    "remeda": "^2.2.2",
    "ts-key-enum": "^2.0.12",
    "ts-pattern": "^5.2.0",
    "vee-validate": "^4.13.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue3-apexcharts": "^1.5.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/utils": "^0.7.8",
    "@commitlint/config-angular": "18.4.3",
    "@playwright/test": "^1.42.1",
    "@rushstack/eslint-patch": "^1.7.2",
    "@tanstack/vue-query": "^5.51.3",
    "@tanstack/vue-table": "^8.17.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^20.11.24",
    "@types/numeral": "^2.0.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.18",
    "commitlint": "18.4.3",
    "cross-env": "^7.0.3",
    "cz-git": "^1.9.4",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "fast-glob": "^3.3.2",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "naive-ui": "^2.38.2",
    "npm-run-all2": "^6.1.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "1.69.5",
    "standard-version": "^9.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vite-plugin-pages": "^0.32.2",
    "vite-plugin-vue-devtools": "^7.0.21",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.3"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "pnpm run lint",
    "*.{ts,tsx,vue,css}": "pnpm run format"
  }
}
